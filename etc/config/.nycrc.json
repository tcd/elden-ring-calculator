{
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": false,
    "all": true,
    "extension": [
        ".ts"
    ],
    "include": [
        "src/**/*.ts"
    ],
    "reporter": [
        "html",
        "lcov",
        "text",
        "text-summary"
    ],
    "report-dir": "etc/coverage"
}
